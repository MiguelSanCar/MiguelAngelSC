<Window x:Class="WPF_Pruebas3.FDatePicker"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_Pruebas3"
        mc:Ignorable="d"
        Title="FDatePicker" Height="450" Width="800">

    <DatePicker Width="150" Height="25" SelectedDate="10/04/2023" SelectedDateFormat="long">
        <DatePicker.BlackoutDates>
            <CalendarDateRange Start="10/01/2023" End="10/02/2023"/>
        </DatePicker.BlackoutDates>
    </DatePicker>
    
</Window>
